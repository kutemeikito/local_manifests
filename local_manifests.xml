<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  
  <remote name="ryz"
          fetch="https://github.com/kutemeikito" />

  <remote name="gitlab"
          fetch="https://gitlab.com/" />

  <remote name="github"
          fetch="https://github.com/" />

  <remote name="Arrow"
          fetch="https://github.com/ArrowOS" />

  <!-- Device Tree & Vendor Tree -->
  <project path="device/xiaomi/ginkgo" name="device_xiaomi_ginkgo" remote="ryz" revision="fourteen/derp" />
  <project path="vendor/xiaomi/ginkgo" name="vendor_xiaomi_ginkgo" remote="ryz" revision="udc" />

  <!-- replace Derpfest frameworks -->
  <remove-project name="frameworks_base"/>
  <project path="frameworks/base" name="frameworks_base-derp" remote="ryz" revision="14" />
  <remove-project name="frameworks_av"/>
  <project path="frameworks/av" name="frameworks_av-derp" remote="ryz" revision="14" />
  <remove-project name="vendor_derp"/>
  <project path="vendor/derp" name="vendor_derp" remote="ryz" revision="14" />
  <remove-project name="system_core"/>
  <project path="system/core" name="system_core-derp" remote="ryz" revision="14" />

  <!-- Kernel & Clang -->
  <project path="kernel/xiaomi/ginkgo" name="android_kernel_xiaomi_ginkgo" remote="ryz" revision="fourteen/main" />
  <project path="prebuilts/clang/host/linux-x86/clang-rastamod" name="kutemeikito/RastaMod69-Clang" remote="gitlab" revision="clang-20.0" />

  <!-- MIUI Camera -->
  <project path="vendor/miuicamera" name="ArrowOS-Devices/android_vendor_miuicamera" remote="github" revision="arrow-12.0-a3" />
</manifest>
